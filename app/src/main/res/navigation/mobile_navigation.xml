<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.semaproject.chatapp2021.ui.home.HomeFragment"
        android:label="@string/title_subject"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.semaproject.chatapp2021.ui.dashboard.DashboardFragment"
        android:label="@string/title_chat"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.semaproject.chatapp2021.ui.notifications.ProfileFragment"
        android:label="@string/title_setting"
