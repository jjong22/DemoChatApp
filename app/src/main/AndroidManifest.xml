        android:label="공부조이고"
        android:roundIcon="@mipmap/ic_culture_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChatApp2021">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ExcelActivity"
            android:exported="true" />
        <activity android:name=".TutorialActivity"></activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity
            android:name=".TapActivity"
            android:label="@string/title_activity_tap" />
